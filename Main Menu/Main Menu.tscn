script/source = "extends Container

var save_data

func _ready():
	save_data = game.load_game(name)
	if not save_data:
		get_node('Info').set_text(\"Empty\")
		get_node('Info').set_anchors_and_margins_preset(PRESET_CENTER)
	if save_data:
		get_node('Info').set_text(save_data['arcology']['name']+\"  |  \"+save_data['index']['os_date']+\", \"+save_data['index']['os_time'])
		get_node('Info').set_anchors_and_margins_preset(PRESET_CENTER)

func _on_mouse_entered():
	get_node('Top').set_modulate('8cff7200')
	get_node('Bot').set_modulate('8cff7200')

func _on_mouse_exited():
	get_node('Top').set_modulate('64ffffff')
	get_node('Bot').set_modulate('64ffffff')

func _on_pressed():
	var dg = get_node('ConfirmationDialog')
	if save_data:
		dg.set_text(\"\\n\"+
			\"Slot \"+name+\" contains save data:\"+\"\\n\"+
			\"\\n\"+
			save_data['arcology']['name']+\"  |  \"+save_data['index']['os_date']+\", \"+save_data['index']['os_time']+\"\\n\"+
			\"\\n\"+
			\"Would you like to overwrite this data with a new game?\")
		dg.popup_centered()
	else:
		game.data_slot = name
		game.queue_save = true
		get_tree().change_scene(\"res://Game.tscn\")

func _on_ConfirmationDialog_confirmed():
	game.delete_game(name)
	game.data_slot = name
	game.queue_save = true
	get_tree().change_scene(\"res://Game.tscn\")
"

[sub_resource type="GDScript" id=4]
script/source = "extends AcceptDialog

func _ready():
	if not game.config.get_value(\"confirmation\", \"skip\") == true:
		call_deferred('popup')

func _on_ConfirmationDialog_confirmed():
	hide()
"

