script/source = "extends Control

onready var s = get_node('../../../')

func _ready():
	get_tree().get_root().get_node('Game/Clock').connect('timeout',self,'_set_gauge')
	_set_gauge()

func _set_gauge():
	get_node('Value').set_text(str(int(s.health)))
	for nodes in get_node('Fill').get_children():
		nodes.hide()
	if s.health >= 100:
		get_node('Fill/overlay').show()
	else:
		get_node('Fill/overlay').hide()
	get_node('Fill/'+_round()).show()
	get_node('Fill/'+_round()).set_modulate(_color())

func _round():
	if s.health >= 100:
		return str(int(round(s.health/10)*10)-100)
	else:
		return str(int(round(s.health/10)*10))

func _color():
	var color
	if s.health <= 10:
		color = \"ff0000\"
	elif s.health < 20:
		color = \"ff4500\"
	elif s.health < 30:
		color = \"ffa500\"
	elif s.health < 40:
		color = \"ffd100\"
	elif s.health < 50:
		color = \"ffff00\"
	elif s.health < 60:
		color = \"9acd32\"
	elif s.health < 70:
		color = \"5bfb5b\"
	elif s.health < 80:
		color = \"00ff00\"
	elif s.health < 90:
		color = \"00ca00\"
	elif s.health < 100:
		color = \"009e00\"
	elif s.health < 110:
		color = \"008000\"
	elif s.health < 120:
		color = \"008032\"
	elif s.health < 130:
		color = \"00bc6c\"
	elif s.health < 140:
		color = \"00e5a4\"
	elif s.health < 150:
		color = \"00e4c4\"
	elif s.health < 160:
		color = \"00ffed\"
	elif s.health < 170:
		color = \"00edff\"
	elif s.health < 180:
		color = \"00c9ff\"
	elif s.health < 190:
		color = \"00a5ff\"
	elif s.health >= 190:
		color = \"0096ff\"
	return(color)

func _on_mouse_entered():
	get_node('Title').show()
	get_node('Value').show()
	get_node('Icon').hide()

func _on_mouse_exited():
	get_node('Title').hide()
	get_node('Value').hide()
	get_node('Icon').show()
