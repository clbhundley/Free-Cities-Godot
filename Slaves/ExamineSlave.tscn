script/source = "extends RichTextLabel

func remove_lines(amount):
	var lower_text = [
		get_node(\"../Appearance\"),
		get_node(\"../Sexuality\"),
		get_node(\"../Skills\"),
		get_node(\"../Traits\")]
	for node in lower_text:
		var pos_x = node.rect_position.x
		var pos_y = node.rect_position.y
		node.set_position(Vector2(pos_x,pos_y-(32*amount)),true)

func set_stats(_slave):
	var intelligence = _slave.stats._intelligence()
	if intelligence != \"\":
		parse_bbcode(
		str(_slave.age)+\", \"+_slave.ethnicity+\", \"+_slave.gender+\"\\n\"+
		_slave.stats._intelligence()+\"\\n\"+
		_slave.stats._devotion()+\", \"+_slave.stats._trust())
	else:
		parse_bbcode(
		str(_slave.age)+\", \"+_slave.ethnicity+\", \"+_slave.gender+\"\\n\"+
		_slave.stats._devotion()+\", \"+_slave.stats._trust())
		remove_lines(1)
"

[sub_resource type="GDScript" id=6]
script/source = "extends RichTextLabel

func adjust_lines(amount):
	var lower_text = [
		get_node(\"../Sexuality\"),
		get_node(\"../Skills\"),
		get_node(\"../Traits\")]
	for node in lower_text:
		var pos_x = node.rect_position.x
		var pos_y = node.rect_position.y
		node.set_position(Vector2(pos_x,pos_y-(30*amount)),true)

func set_stats(_slave):
	var string = \"\"
	var face = _slave.get_node(\"Scripts/Stats\")._face()
	if face != \"\":
		string += face
		if _slave.flags.has(\"weight\"):
			string += \", \"
			string += _slave.flags[\"weight\"]+\"\\n\"
		else:
			string += \"\\n\"
	elif _slave.flags.has(\"weight\"):
		string += _slave.flags[\"weight\"]+\"\\n\"
	else:
		adjust_lines(1)
	
	if _slave.flags.has(\"body\"):
		string += _slave.flags[\"body\"]
		if _slave.flags.has(\"physique\"):
			string += \", \"
			string += _slave.flags[\"physique\"]+\"\\n\"
		else:
			string += \"\\n\"
	elif _slave.flags.has(\"physique\"):
		string += _slave.flags[\"physique\"]+\"\\n\"
	else:
		adjust_lines(1)
	
	if _slave.flags.has(\"gender\"):
		string += _slave.flags[\"gender\"]+\"\\n\"
		adjust_lines(-1)
	
	if _slave.voice != \"\":
		var voice_string = _slave.voice
		voice_string[0] = voice_string[0].capitalize()
		if voice_string != \"Mute\":
			voice_string += \"voice\"
		string += voice_string
	else:
		adjust_lines(1)
	
	if string == \"\":
		adjust_lines(0.6)
	parse_bbcode(string)
"

