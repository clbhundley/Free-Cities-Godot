script/source = "extends Control

var _slave

func _on_mouse_entered():
	get_node('Title').show()
	get_node('Value').show()
	get_node('Icon').hide()

func _on_mouse_exited():
	get_node('Title').hide()
	get_node('Value').hide()
	get_node('Icon').show()

func _set_gauge():
	get_node('Value').set_text(str(int(_slave.health)))
	for nodes in get_node('Fill').get_children():
		nodes.hide()
	if _slave.health >= 100:
		get_node('Fill/overlay').show()
	else:
		get_node('Fill/overlay').hide()
	get_node('Fill/'+_round()).show()
	get_node('Fill/'+_round()).set_modulate(_color())

func _round():
	if _slave.health >= 100:
		return str(int(round(_slave.health/10)*10)-100)
	else:
		return str(int(round(_slave.health/10)*10))

func _color():
	var color
	if _slave.health <= 10:
		color = \"ff0000\"
	elif _slave.health < 20:
		color = \"ff4500\"
	elif _slave.health < 30:
		color = \"ffa500\"
	elif _slave.health < 40:
		color = \"ffd100\"
	elif _slave.health < 50:
		color = \"ffff00\"
	elif _slave.health < 60:
		color = \"9acd32\"
	elif _slave.health < 70:
		color = \"5bfb5b\"
	elif _slave.health < 80:
		color = \"00ff00\"
	elif _slave.health < 90:
		color = \"00ca00\"
	elif _slave.health < 100:
		color = \"009e00\"
	elif _slave.health < 110:
		color = \"008000\"
	elif _slave.health < 120:
		color = \"008032\"
	elif _slave.health < 130:
		color = \"00bc6c\"
	elif _slave.health < 140:
		color = \"00e5a4\"
	elif _slave.health < 150:
		color = \"00e4c4\"
	elif _slave.health < 160:
		color = \"00ffed\"
	elif _slave.health < 170:
		color = \"00edff\"
	elif _slave.health < 180:
		color = \"00c9ff\"
	elif _slave.health < 190:
		color = \"00a5ff\"
	elif _slave.health >= 190:
		color = \"0096ff\"
	return(color)
"

