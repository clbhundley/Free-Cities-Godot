	_on_mouse_entered()
	_on_mouse_exited()

func _on_pressed():
	var g = get_tree().get_root().get_node('Game')
	for node in g.get_children():
		if node.get_class() != \"Node\" and not node is Timer:
			node.hide()
	g.get_node('Arcology/Display').hide()
#	g.get_node('Finance').show()
#	g.get_node('Finance').reset_scene()
	g.get_node('GUI').show()
	if get_node('../../AI Panel').is_visible_in_tree():
		get_node('../..')._on_AI_pressed()
	for tab in get_parent().get_children():
		tab._on_mouse_exited()

func _on_mouse_entered():
	set_self_modulate('782873c8')

func _on_mouse_exited():
#	if not get_node('../../../Finance').is_visible_in_tree():
		set_self_modulate('3c333a4b')
"

[sub_resource type="GDScript" id=27]
script/source = "extends TextureRect

func _ready():
	_on_mouse_entered()
	_on_mouse_exited()

func _on_pressed():
	var _game = get_tree().get_root().get_node('Game')
	var arcology = _game.get_node('Arcology')
	var side_panel = get_node(\"../../SidePanel\")
	var dock = get_node(\"../../Dock\")
	for node in _game.get_children():
		if node.get_class() != \"Node\" and not node is Timer:
			node.hide()
	game.set_bg_color(game.BG_COLOR_DEFAULT)
	var animation = true
	if side_panel.hidden:
		animation = false
		side_panel.open()
		side_panel.get_node(\"ShowPanel\").hide()
	dock.get_node(\"ActionButton\").show()
	dock.get_node(\"SplitButton\").hide()
	if arcology.selected_building:
		dock.set_mode(\"ManageBuilding\",animation)
	elif arcology.selected_terra:
		dock.set_mode(\"ManageTerra\",animation)
	else:
		dock.set_mode(\"ManageArcology\",animation)
	get_node(\"../../Header/Slaves\").hide()
	get_node(\"../../Header/Arcology\").show()
	arcology.show()
	if arcology.selected_terra:
		arcology.get_node('ChangeView').show()
	SlaveUtils.reset_active_slaves_visibility()
	for node in get_tree().get_nodes_in_group(\"Slave UI\"):
		node.hide()
	arcology.get_node('Cambase/Camera').current = true
	_game.get_node('GUI').show()
	if get_node('../../AI Panel').is_visible_in_tree():
		get_node('../..')._on_AI_pressed()
	for tab in get_parent().get_children():
		tab._on_mouse_exited()

func _on_mouse_entered():
	set_self_modulate('782873c8')

func _on_mouse_exited():
	if not get_node('../../../Arcology').is_visible_in_tree():
		set_self_modulate('3c333a4b')
"

[sub_resource type="GDScript" id=28]
script/source = "extends TextureRect

func _ready():
	_on_mouse_entered()
	_on_mouse_exited()

func _on_pressed():
	var _game = get_tree().get_root().get_node('Game')
	var slaves = _game.get_node('Slaves')
	var side_panel = get_node(\"../../SidePanel\")
	var dock = get_node(\"../../Dock\")
	for node in _game.get_children():
		if node.get_class() != \"Node\" and not node is Timer:
			node.hide()
	game.set_bg_color(game.BG_COLOR_DEFAULT)
	var animation = true
	if side_panel.hidden:
		animation = false
		side_panel.open()
		side_panel.get_node(\"ShowPanel\").hide()
	dock.get_node(\"ActionButton\").show()
	dock.get_node(\"SplitButton\").hide()
	get_node(\"../../Dock\").set_mode(\"ManageSlaves\",animation)
	get_node(\"../../Header/Slaves\").show()
	get_node(\"../../Header/Arcology\").hide()
	_game.get_node('Arcology/ChangeView').hide()
	SlaveUtils.reset_active_slaves_visibility()
	slaves.get_node('Camera').current = true
	slaves.show()
	_game.get_node('GUI').show()
	slaves.set_active_collection(\"Owned\",false)
	#slaves.clamp_camera_position()
	slaves.slide_camera()
	if get_node('../../AI Panel').is_visible_in_tree():
		get_node('../..')._on_AI_pressed()
	for tab in get_parent().get_children():
		tab._on_mouse_exited()

func _on_mouse_entered():
	set_self_modulate('782873c8')

func _on_mouse_exited():
	if not get_node('../../../Slaves').is_visible_in_tree():
		set_self_modulate('3c333a4b')
"

