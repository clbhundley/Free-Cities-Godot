[sub_resource type="GDScript" id=22]
script/source = "extends Control

onready var timer = get_tree().get_root().get_node('Game/Clock')

func _ready():
	_on_ClockSpeed_value_changed($Sliders/ClockSpeed.value)
	_on_TimeScale_value_changed($Sliders/TimeScale.value)

func _on_ClockSpeed_value_changed(value):
	timer.stop()
	var speed = value/100
	timer.wait_time = 1-speed
	timer.start()
	var display_value = str(stepify(1/timer.wait_time,0.01))
	$Sliders/ClockSpeed/Label.set_text(\"Clock Speed: x\"+display_value)

func _on_TimeScale_value_changed(value):
	timer.stop()
	time.scale = value
	timer.start()
	var display_value = str(stepify(value,0.01))
	$Sliders/TimeScale/Label.set_text(\"Time Scale: x\"+display_value)
"

