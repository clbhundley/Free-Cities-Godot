script/source = "extends Control

onready var side_panel = get_node(\"../SidePanel\")
onready var slaves = get_tree().get_root().get_node(\"Game/Slaves\")

var action_button_text
var mode

func _ready():
	get_tree().get_root().connect('size_changed',self,'resize')
	mode = \"ManageSlaves\"
	set_mode(mode)

func set_mode(new_mode,animation=true):
	match new_mode:
		\"ManageSlave\":
			action_button_text = \"Manage Slave\"
			if side_panel.open:
				$ActionButton.set_text(\"Close\")
			else:
				$ActionButton.set_text(action_button_text)
		\"ManageSlaves\":
			action_button_text = \"Manage Slaves\"
			if side_panel.open:
				$ActionButton.set_text(\"Close\")
			else:
				$ActionButton.set_text(action_button_text)
		\"PurchaseSlaves\":
			action_button_text = \"Back\"
			$ActionButton.set_text(action_button_text)
		\"KidnappersMarket\":
			game.set_bg_color(game.BG_COLOR_PLUMB)
			action_button_text = \"Back\" #old
			$ActionButton.set_text(action_button_text) #old
			$ActionButton.hide()
			$SplitButton.show()
			slaves.set_active_collection(\"Kidnappers Market\")
		\"NeighboringArcologies\":
			game.set_bg_color(game.BG_COLOR_DEEP)
			action_button_text = \"Back\" #old
			$ActionButton.set_text(action_button_text) #old
			$ActionButton.hide()
			$SplitButton.show()
			slaves.set_active_collection(\"Neighboring Arcologies\")
		\"ManageArcology\":
			action_button_text = \"Manage Arcology\"
			if side_panel.open:
				$ActionButton.set_text(\"Close\")
			else:
				$ActionButton.set_text(action_button_text)
		\"ManageTerra\":
			action_button_text = \"Manage Terra\"
			if side_panel.open:
				$ActionButton.set_text(\"Close\")
			else:
				$ActionButton.set_text(action_button_text)
		\"ManageBuilding\":
			action_button_text = \"Manage Building\"
			if side_panel.open:
				$ActionButton.set_text(\"Close\")
			else:
				$ActionButton.set_text(action_button_text)
	get_content(new_mode).update()
	side_panel.change_content(get_content(mode),get_content(new_mode),animation)
	mode = new_mode

func get_content(content):
	return side_panel.get_node(content)

func _on_ActionButton_pressed():
	var animation = true
	if not side_panel.open:
		animation = false
	if side_panel.rect_size.x > margin_right+30:
		side_panel.rect_size.x = margin_right+30
	if mode == \"PurchaseSlaves\":
		mode = \"ManageSlaves\"
		game.set_bg_color(game.BG_COLOR_DEFAULT)
		slaves.set_active_collection(\"Owned\",false)
		side_panel.change_content(
			get_content(\"PurchaseSlaves\"),
			get_content(mode))
		action_button_text = \"Manage Slaves\"
		$ActionButton.set_text(\"Close\")
	elif mode == \"KidnappersMarket\":
		mode = \"PurchaseSlaves\"
		game.set_bg_color(game.BG_COLOR_DEFAULT)
		slaves.set_active_collection(\"Owned\")
		side_panel.get_node(\"ShowPanel\").hide()
		side_panel.change_content(
			get_content(\"KidnappersMarket\"),
			get_content(mode),
			animation)
		if not side_panel.open:
			side_panel.open()
		$ActionButton.show()
		$SplitButton.hide()
		action_button_text = \"Back\"
		$ActionButton.set_text(action_button_text)
	elif mode == \"NeighboringArcologies\":
		mode = \"PurchaseSlaves\"
		game.set_bg_color(game.BG_COLOR_DEFAULT)
		slaves.set_active_collection(\"Owned\")
		side_panel.get_node(\"ShowPanel\").hide()
		side_panel.change_content(
			get_content(\"NeighboringArcologies\"),
			get_content(mode),
			animation)
		if not side_panel.open:
			side_panel.open()
		$ActionButton.show()
		$SplitButton.hide()
		action_button_text = \"Back\"
		$ActionButton.set_text(action_button_text)
	elif side_panel.open:
		side_panel.close()
	else:
		side_panel.open()

func _on_Close_pressed():
	game.set_bg_color(game.BG_COLOR_DEFAULT)
	$ActionButton.show()
	$SplitButton.hide()
	if side_panel.open:
		side_panel.close()
	slaves.set_active_collection(\"Owned\")
	set_mode(\"ManageSlaves\",false)

var rubik_44 = preload('res://Fonts/Rubik-Light-44.tres')
var rubik_40 = preload('res://Fonts/Rubik-Light-40.tres')
var rubik_36 = preload('res://Fonts/Rubik-Light-36.tres')
var rubik_30 = preload('res://Fonts/Rubik-Light-30.tres')
var rubik_24 = preload('res://Fonts/Rubik-Light-24.tres')
func scale_fonts():
	var scale_adjusted = display.scale * 1.6
	var scale_rubik_24 = display.scale_x * 33
	var scale_rubik_30 = display.scale_x * 35
	var scale_rubik_36 = display.scale_x * 40
	var scale_rubik_40 = display.scale_x * 45
	var scale_rubik_44 = display.scale_x * 50
	rubik_24.size = clamp(scale_rubik_24, 20, 30)
	rubik_30.size = clamp(scale_rubik_30, 28, 35)
	rubik_36.size = clamp(scale_rubik_36, 26, 39)
	rubik_40.size = clamp(scale_rubik_40, 28, 42)
	rubik_44.size = clamp(scale_rubik_44, 34, 49)

func resize():
	scale_fonts()
	var scale_adjusted = display.scale * 1.6
	var scale_rubik_24 = display.scale_x * 33
	$AI.margin_right = min(110, 110 * scale_adjusted)
	$AI.margin_top = max(-1 * 110, -1 * 110 * scale_adjusted)
	margin_right = min(430, 430 * scale_adjusted)
	margin_top = max(-110, -110 * scale_adjusted)
	side_panel.margin_right = margin_right + 30
	if not side_panel.open:
		side_panel.hide()
	$ActionButton.get('custom_fonts/font').size = min(max(scale_rubik_24,18),24)
	for node in side_panel.get_children(): #refreshing nodes by adding 0 to margin
		if node is VBoxContainer:
			node.margin_left += 0
	$ActionButton.margin_left += 0
	$SplitButton.margin_left += 0
"

[sub_resource type="GDScript" id=23]
script/source = "extends Control

#RPG style dialogue
#not currently in use

var halted
var counter = 0

func say(string):
	halted = false
	$Text.push_align(0)
	while not string.empty() and not halted:
		var c = string[0]
		string.erase(0,1)
		$Text.add_text(c)
		if not c == \" \" and counter == 1:
			$Audio.play()
		if c == \",\":
			$Timer.wait_time = 0.1
		elif c == \".\" or c == \"!\" or c == \"?\":
			$Timer.wait_time = 0.2
		else:
			$Timer.wait_time = 0.03
		$Timer.start()
		yield($Timer,\"timeout\")
		if counter == 0:
			counter = 1
		else:
			counter = 0
	$Text.newline()

func read(file):
	var text
	var f = File.new()
	f.open(file,File.READ)
	text = f.get_as_text()
	f.close()
	$Text.add_text(text)
	say(text)

func halt():
	halted = true
"

