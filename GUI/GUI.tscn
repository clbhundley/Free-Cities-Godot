script/source = "extends VBoxContainer

func _ready():
	pass # Replace with function body.

func update():
	var selected_building = ArcUtils.get_arcology().selected_building
	var selected_building_name = ArcUtils.sector_name(selected_building.name)
	if selected_building_name[-2] == \"_\":
		selected_building_name = selected_building_name.rstrip(\" a\")
	get_node(\"Title\").set_text(selected_building_name.capitalize()+\":\")
	if \"owned\" in selected_building:
		if selected_building.owned:
			#daily upkeep
			get_node(\"Buy\").hide()
			get_node(\"Sell\").show()
			get_node(\"Construction\").show()
	else:
		get_node(\"Buy\").show()
		get_node(\"Sell\").hide()
		get_node(\"Construction\").hide()
"

