[sub_resource type="GDScript" id=35]
script/source = "extends Panel

func _pause():
	if self.is_visible_in_tree():
		get_tree().set_pause(false)
		self.hide()
		get_node('Saves').hide()
		get_node('Options').hide()
	else:
		get_tree().set_pause(true)
		self.show()

func _on_SaveLoad_pressed():
	get_node('Saves').show()

func _on_Options_pressed():
	get_node('Options').show()

func _on_Main_Menu_pressed():
	_pause()
	data.save_game(data.save_slot)
	get_tree().change_scene('res://Main Menu/Main Menu.tscn')

func _input(event):
	if event.is_action_pressed('ui_cancel') or event.is_action_pressed('ui_back'):
		if get_node('Saves').is_visible_in_tree():
			get_node('Saves').hide()
		elif get_node('Options').is_visible_in_tree():
			get_node('Options').hide()
		elif event.is_action_pressed('ui_cancel'):
			_pause()

func _on_Menu_Button_pressed():
	_pause()
"

